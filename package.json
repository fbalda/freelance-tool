{
  "name": "freelance-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:start": "docker-compose -f ./devdb/docker-compose.yaml up -d && prisma generate && dotenv -e .env.local -- prisma migrate dev && dotenv -e .env.local -- prisma db push",
    "testdb:start": "docker-compose -f ./testdb/docker-compose.yaml up -d -V --force-recreate && prisma generate && dotenv -e .env.local -e .env -- prisma migrate dev && dotenv -e .env.local -e .env -- prisma db push",
    "testserver": "npm run testdb:start && next dev",
    "dev": "npm run db:start && next dev",
    "build": "prisma generate && next build",
    "start": "dotenv -e .env.local -e .env -- prisma migrate deploy && dotenv -e .env.local -e .env -- prisma db push && next start",
    "lint": "next lint",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test": "start-server-and-test testserver http://localhost:3000 cypress:headless",
    "format": "prettier --write ./src ./cypress"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@types/express": "^4.17.14",
    "@types/nodemailer": "^6.4.6",
    "@types/pdfkit": "^0.12.8",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.3",
    "dotenv-cli": "^6.0.0",
    "iron-session": "^6.3.1",
    "next": "13.0.0",
    "nodemailer": "^6.8.0",
    "otplib": "^12.0.1",
    "pdfkit": "^0.13.0",
    "pdfkit-table": "^0.1.99",
    "prisma-client": "^0.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.6.0",
    "react-query": "^3.39.2",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^10.11.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "i": "^0.3.7",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prisma": "^4.6.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.2",
    "typescript": "4.8.4"
  }
}
